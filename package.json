{
  "name": "twice-polls",
  "version": "0.1.1",
  "private": true,
  "author": "Ralph Largo",
  "description": "Unofficial twice fans (onces) voting app made with nextjs, tailwind and firestore",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "import:db": "firebase emulators:start --import @localdb",
    "export:db": "firebase emulators:export @localdb",
    "stopall": "lsof -t -i:2000 -i:6001 -i:9080 -i:8086 -i:9099 | xargs kill -9",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/react": "^6.2.5",
    "@sentry/tracing": "^6.2.5",
    "@tailwindcss/postcss7-compat": "^2.0.4",
    "@types/react-redux": "^7.1.16",
    "@types/styled-components": "^5.1.9",
    "autoprefixer": "^10.2.5",
    "babel-plugin-styled-components": "^1.12.0",
    "chart.js": "^3.0.2",
    "cookie": "^0.4.1",
    "dayjs": "^1.10.4",
    "firebase": "^8.3.2",
    "firebase-admin": "^9.6.0",
    "firebase-functions": "^3.13.2",
    "framer-motion": "^4.1.2",
    "husky": "^6.0.0",
    "js-cookie": "^2.2.1",
    "next": "10.1.3",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.9",
    "react": "17.0.2",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "17.0.2",
    "react-firebaseui": "^4.1.0",
    "react-hookedup": "^0.1.2",
    "react-redux": "^7.2.3",
    "styled-components": "^5.2.3",
    "sweetalert2": "^10.15.7",
    "swr": "^0.5.5",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.0.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "postcss-preset-env": "^6.7.0",
    "standard-version": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
